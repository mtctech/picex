{
	"name": "picex",
	"version": "0.1.0",
	"description": "Picex is a simple web image editor based on React, Fabricjs and Tailwindcss.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"dist": "vite build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"prettier-eslint --write"
		]
	},
	"keywords": [],
	"authors": [
		{
			"name": "Mox",
			"email": "hexmox@gmail.com"
		}
	],
	"license": "GPL-3.0-only",
	"dependencies": {
		"@uiw/color-convert": "^2.4.0",
		"@uiw/react-color-chrome": "^2.3.4",
		"@uiw/react-color-sketch": "^2.3.4",
		"@uiw/react-color-wheel": "^2.3.4",
		"ahooks": "^3.8.4",
		"antd": "^5.0.0",
		"clsx": "^2.1.1",
		"downloadjs": "^1.4.7",
		"fabric": "^6.5.4",
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"react-icons": "^5.4.0",
		"tailwind-merge": "^2.6.0",
		"uuid": "^11.0.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@types/downloadjs": "^1.4.6",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"postcss": "^8.5.1",
		"prettier": "^3.3.3",
		"prettier-eslint": "^16.3.0",
		"prettier-eslint-cli": "^8.0.1",
		"semantic-release": "^24.2.0",
		"tailwindcss": "^3.4.17",
		"utility-types": "^3.11.0",
		"vite": "^6.0.7",
		"vite-plugin-dts": "^4.5.0",
		"vite-plugin-externalize-deps": "^0.9.0",
		"vite-plugin-svgr": "^4.3.0",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"files": [
		"src",
		"dist",
		"package.json"
	],
	"volta": {
		"node": "20.18.1"
	}
}
